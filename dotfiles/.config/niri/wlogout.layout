{
    "label" : "lock",
    "action" : "loginctl lock-session",
    "text" : "Lock",
    "keybind" : "l"
}
{
    "label" : "logout",
    "action" : "niri msg action quit",
    "text" : "Log Out",
    "keybind" : "e"
}
{
    "label" : "suspend",
    "action" : "systemctl suspend",
