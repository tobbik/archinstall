output eDP-1 {
  mode       1920x1080@60Hz
  position   0,0
  scale      1.25
}

output DP-1 {
  mode       1920x1080@60Hz
  position   0,0
  scale      1.5
}

output DP-2 {
  mode       2560x1440@60Hz
  position   3440,0
  transform  270
}

profile home_1 {
  #output HDMI-A-1 position 1366,0
  #output eDP-1 position 0,0
  output eDP-1 enable
  output DP-1  disable
}

profile work {
  #output HDMI-A-1 position 1366,0
  #output eDP-1 position 0,0
  output eDP-1 enable
  output DP-1  enable
}
